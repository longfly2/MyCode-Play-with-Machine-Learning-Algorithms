   "source": [
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "def entropy_2clf(p):\n",
    "    return - p * np.log(p) - (1-p) * np.log(1-p)\n",
    "\n",
    "x = np.linspace(0.001, 0.999, 1000)\n",
    "\n",
    "plt.plot(x, entropy_2clf(x))"
   ]
